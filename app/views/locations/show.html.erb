<p id="notice"><%= notice %></p>

<p>
  <strong>Tent type:</strong>
  <%= @location.tent_type %>
</p>

<p>
  <strong>Jockey box:</strong>
  <%= @location.jockey_box %>
</p>

<p>
  <strong>Bin:</strong>
  <%= @location.bin %>
</p>

<p>
  <strong>Tables:</strong>
  <%= @location.tables %>
</p>

<p>
  <strong>Completed:</strong>
  <%= @location.completed %>
</p>

<p>
  <strong>Notes:</strong>
  <%= @location.notes %>
</p>

<p>
  <strong>Long:</strong>
  <%= @location.long %>
</p>

<p>
  <strong>Lat:</strong>
  <%= @location.lat %>
</p>

<p>
  <strong>Beers at this Tent</strong>
  <% @location.beers.each do |beer| %>
  <li><%= beer.product %></li>
<% end %>
</p>

<div id="map"></div>
    <script type="text/javascript">

        var map;
        function initMap() {
          map = new google.maps.Map(document.getElementById('map'), {
            center: {
              lat: <%= @location.lat %>,
              lng: <%= @location.long %>
            },
            zoom: 18,
            mapTypeId: google.maps.MapTypeId.SATELLITE
          });
        }

    </script>

<%= link_to 'Edit', edit_location_path(@location) %> |
<%= link_to 'Back', locations_path %>
